{
  "extends": ["airbnb-typescript/base"],
  "parserOptions": {
      "project": ["./tsconfig.json"]
  },
  "rules": {
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "max-len": 0,
    "no-console": "off"
  }
}
